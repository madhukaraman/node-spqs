{
  "name": "node-spqs",
  "version": "1.0.0",
  "description": "A priority queue service built on top of AWS SQS with Redis for priority management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint 'src/**/*.ts'",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run lint && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "aws",
    "sqs",
    "priority-queue",
    "redis",
    "queue",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madhukaraman/node-spqs.git"
  },
  "bugs": {
    "url": "https://github.com/madhukaraman/node-spqs/issues"
  },
  "homepage": "https://github.com/madhukaraman/node-spqs#readme",
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@types/redis": "^4.0.10",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.1",
    "typescript": "^5.8.2"
  },
  "files": [
    "dist/**/*"
  ]
}
